@model PostType

@{
    ViewData["Title"] = Model.Title;
}

<div class="row ">
<div class="col-md-5 au-card recent-report">
<h2>PostTypes Details</h2>
 </div>
 </div>

<div>
     <dl class="row col-md-5 au-card recent-report">
        <dt class = "col-sm-5">
            @Html.DisplayNameFor(model => model.Title)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt class = "col-sm-5">
            @Html.DisplayNameFor(model => model.Code)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
        <dt class="col-sm-5">
            Taxonomies
        </dt>
        <dd class="col-sm-10">
            <table>
                @foreach (var i in ViewBag.isChecked)
                {
                    <tr>
                        <td>
                            <label class="control-label">@i.Taxonomy.Name</label>
                        </td>
                    </tr>
                }
            </table>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.PostTypeId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
